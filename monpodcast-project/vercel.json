{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      },
      {
        "src": "server/**",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/IA/(.*)",
        "dest": "server.py"
      },
      {
        "src": "/(.*)",
        "dest": "client/build/$1"
      }
    ]
  }