{
    "version": 2,
    "builds": [
      { "src": "build/**", "use": "@vercel/static" },
      { "src": "server.py", "use": "@vercel/IA" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "http://localhost:5000/IA/my-endpoint" },
      { "src": "/api/(.*)", "dest": "http://localhost:5000/my-endpoint" }
    ]
  }